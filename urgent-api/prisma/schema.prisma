// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema


generator client {
  provider = "prisma-client-js"
  previewFeatures = ["tracing"]
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_URL")
}

model Urgent {
  id      Int    @id @default(autoincrement())
  user_id Int
  task_id Int
}